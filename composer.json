{
    "name": "decimalteam/decimal-php-sdk",
    "description": "decimal sdk",
    "version": "0.14.02",
    "license": "MIT",
    "authors": [
        {
            "name": "DecimalTeam",
            "role": "dDeveloper"
        }
    ],
    "require": {
        "fgrosse/phpasn1": "^2.1",
        "bitwasp/bech32": "^0.0.1",
        "guzzlehttp/guzzle": "~6.5|^7",
        "kornrunner/keccak": "^1.1",
        "simplito/elliptic-php": "^1.0",
        "php": "^7.3|8.*",
        "google/protobuf": "^3.21",
        "bitcoin-php/bitcoin-ecdsa": "^1.3",
        "grpc/grpc": "~1.30.0",
        "web3p/ethereum-tx": "^0.4.3",
        "protobuf-php/protobuf": "^0.1.3",
        "web3p/web3.php": "0.1.6",
        "ext-json": "*",
        "mdanter/ecc": "^1.0",
        "kornrunner/secp256k1": "^0.2.0",
        "protonlabs/bitcoin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "autoload": {
        "psr-4": {
            "DecimalSDK\\": "src/",
            "Decimal\\": "src/Utils/Proto/Decimal/",
            "Cosmos\\": "src/Utils/Proto/Cosmos/",
            "GPBMetadata\\": "src/Utils/Proto/GPBMetadata/",
            "Ethermint\\": "src/Utils/Proto/Ethermint/",
            "Gogoproto\\": "src/Utils/Proto/Gogoproto",
            "CosmosProto\\": "src/Utils/Proto/CosmosProto",
            "Tendermint\\": "src/Utils/Proto/Tendermint",
            "Google\\": "src/Utils/Proto/Google"
        },

        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phabel/phabel": true
        }
    }
}
